<record id="collection-object" is-extension="true">
	<services-record-path id="ucjeps">collectionobjects_ucjeps:http://collectionspace.org/services/collectionobject/local/ucjeps,collectionobjects_ucjeps</services-record-path>

	<section id="coreInformation">
		<!-- Remove mini -->
		<field id="nametitle" mini=""/>
	</section>

	<section id="identificationInformation">
		<field id="objectNumber">
			<options>
				<!-- Add UC and JEPS id generators -->
				<option id="uc-accession" sample="UC100000">UC Accession</option>
				<option id="jeps-accession" sample="JEPS100000">Jepson Accession</option>
			</options>
		</field>

		<repeat id="otherNumberList/otherNumber">
			<field id="numberType">
				<options>
					<option id="" default="yes">Please select a value</option>
					<option id="Unknown">Unknown</option>
					<option id="Collector Number">Collector Number</option>
					<option id="Exsiccatae Number">Exsiccatae Number</option>
					<option id="GenBank Code">GenBank Code</option>
					<option id="National Park Service Accession Number">National Park Service Accession Number</option>
					<option id="Nectandra Working Group Number">Nectandra Working Group Number</option>
					<option id="Old institution accession number">Old Institution Accession Number</option>
					<option id="Other Label Number">Other Label Number</option>
					<option id="Page Number">Page Number</option>
					<option id="Taxonomic Serial Number">Taxonomic Serial Number</option>
					<option id="U.C. Botanical Garden Accession Number">U.C. Botanical Garden Accession Number</option>
				</options>
			</field>
		</repeat>
		<field id="collection">
			<options>
				<option id="">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="Jepson Herbarium">Jepson Herbarium</option>
				<option id="University of California Herbarium" default="yes">University Herbarium, University of California, Berkeley (UC)</option>
				<option id="University of California- Santa Barbara">University of California- Santa Barbara</option>
				<option id="Herbarium (UCSC)">Herbarium (UCSC)</option>
				<option id="University of California">University of California</option>
				<option id="DHN">DHN</option>
			</options>
		</field>
		<repeat id="taxonomicIdentGroupList/taxonomicIdentGroup">
			<!-- Add taxon-unverified to autocomplete -->
			<field id="taxon" autocomplete="taxon-taxon,taxon-unverified"/>
			<!-- Change autocomplete to person-person and organization-determination -->
			<field id="identBy" autocomplete="person-person,organization-determination"/>
			<!-- Change autocomplete to institution authority -->
			<field id="institution" autocomplete="organization-institution"/>
		</repeat>
		<repeat id="hybridParentGroupList/hybridParentGroup">
			<!-- Add taxon-unverified to autocomplete -->
			<field id="hybridParent" autocomplete="taxon-taxon,taxon-unverified"/>
		</repeat>
		<!-- Tie labelHeader and labelFooter to Concept Authority -->
		<field id="labelHeader" autocomplete="concept-concept"/>
		<field id="labelFooter" autocomplete="concept-concept"/>
		
		<repeat id="typeSpecimenGroupList/typeSpecimenGroup">
			<!-- Change autocomplete to organization-determination -->
			<field id="typeSpecimenAssertionBy" autocomplete="organization-typeassertion"/>
		</repeat>
		
		<!-- Change autocomplete to institution authority -->
		<field id="donor" autocomplete="organization-institution"/>
	</section>

	<section id="descriptionInformation">
		<field id="phase">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="Cone">Cone</option>
				<option id="Flowering">Flowering</option>
				<option id="Flowering/Fruiting">Flowering/Fruiting</option>
				<option id="Fruiting">Fruiting</option>
				<option id="Gametophyte">Gametophyte</option>
				<option id="Juvenile">Juvenile</option>
				<option id="Reproductive">Reproductive</option>
				<option id="Spores/Sporangia">Spores/Sporangia</option>
				<option id="Sporophyte">Sporophyte</option>
				<option id="Tetrasporophyte">Tetrasporophyte</option>
				<option id="Vegetative (non-reproductive)">Vegetative (non-reproductive)</option>
			</options>
		</field>
		<repeat id="forms">
			<field id="form">
				<options>
					<option id="">Please select a value</option>
					<option id="Unknown">Unknown</option>
					<option id="Mounted on Paper" default="yes">Mounted on Paper</option>
					<option id="Loose in packet">Loose in packet</option>
					<option id="Stored in a Box or Bag">Stored in a Box or Bag</option>
					<option id="Preserved in Liquid">Preserved in Liquid</option>
					<option id="Microscope Slide Preparation">Microscope Slide Preparation</option>
					<option id="Photograph">Photograph</option>
					<option id="Illustration">Illustration</option>
					<option id="Photocopy">Photocopy</option>
					<option id="Box">Box</option>
					<option id="Bag">Bag</option>
					<option id="Map">Map</option>
				</options>
			</field>
		</repeat>
		
		<!-- naturalhistory overrides -->

		<field id="cultivated">
			<!-- Change default to No -->
			<options>
				<option id="">Please select a value</option>
				<option id="true">Yes</option>
				<option id="false" default="yes">No</option>
			</options>
		</field>

		<!-- Local fields -->
		
		<field id="handwritten" section="ucjeps">
			<options>
				<option id="" default="yes">Please select a value</option>
				<option id="Unknown">Unknown</option>
				<option id="yes">Yes</option>
				<option id="no">No</option>
			</options>
		</field>
		<field id="postToPublic" section="ucjeps">
			<options>
				<option id="">Please select a value</option>
				<option id="no">No</option>
				<option id="yes" default="yes">Yes</option>
			</options>
		</field>
	</section>
	
	<section id="objectHistoryAssociationInformation">
		<repeat id="owners">
			<!-- Add organization-institution to autocomplete -->
			<field id="owner" autocomplete="person-person,organization-organization,organization-institution"/>
		</repeat>
		
		<section id="associationInformation">
			<repeat id="assocConceptGroupList/assocConceptGroup">
				<!-- Remove autocomplete -->
				<field id="assocConcept" autocomplete=""/>
			</repeat>
			
			<repeat id="assocOrganizationGroupList/assocOrganizationGroup">
				<!-- Add institution authority to autocomplete -->
				<field id="assocOrganization" autocomplete="organization-organization,organization-institution"/>
			</repeat>
			<repeat id="assocEventOrganizations">
				<!-- Add institution authority to autocomplete -->
				<field id="assocEventOrganization" autocomplete="organization-organization,organization-institution"/>
			</repeat>
		</section>
	</section>
	
	<section id="referenceInformation">
		<repeat id="referenceGroupList/referenceGroup">
			<!-- Remove autocomplete -->
			<field id="reference" autocomplete=""/>
		</repeat>
	</section>

	<section id="objectCollectionInformation">
		<!-- Add ui-search -->
		<field id="fieldCollectionNumber" ui-search="range"/>
		<repeat id="fieldCollectors">
			<!-- Add ui-search, and remove person authority from autocomplete -->
			<field id="fieldCollector" autocomplete="organization-organization" ui-search="repeatable"/>
		</repeat>
	</section>

